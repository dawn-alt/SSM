<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- 打印查询语句 -->
        <setting name="logImpl" value="STDOUT_LOGGING" />
    </settings>

    <!-- 实体类,简称 -设置别名 -->
    <typeAliases>
        <typeAlias alias="SSR_Info" type="YingzuiBean.pojo.SSR_Info" />
        <typeAlias alias="All_Info" type="YingzuiBean.pojo.All_Info" />
        <typeAlias alias="genome" type="YingzuiBean.pojo.genome" />
        <typeAlias alias="primer" type="YingzuiBean.pojo.primer" />
        <typeAlias alias="gene" type="YingzuiBean.pojo.gene" />

    </typeAliases>

    <!-- 实体接口映射文件 -->
    <mappers>
        <mapper resource="mapper/ssrInfoMapper.xml" />
        <mapper resource="mapper/allInfoMapper.xml"/>
        <mapper resource="mapper/genomeMapper.xml"/>
        <mapper resource="mapper/primerMapper.xml"/>
        <mapper resource="mapper/geneMapper.xml"/>
    </mappers>

</configuration>
